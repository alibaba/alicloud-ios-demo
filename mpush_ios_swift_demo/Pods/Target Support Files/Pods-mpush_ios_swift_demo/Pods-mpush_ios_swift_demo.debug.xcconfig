FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/FontAwesome+iOS" "$PODS_CONFIGURATION_BUILD_DIR/KoaPullToRefresh"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/FontAwesome+iOS/FontAwesome_iOS.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/KoaPullToRefresh/KoaPullToRefresh.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "FontAwesome_iOS" -framework "KoaPullToRefresh"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
